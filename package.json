{
  "author": "Nicolas Lochet",
  "name": "wool",
  "description": "Test some idea about event/log based datastorage, CQRS and fancy stuff.",
  "version": "0.2.0",
  "homepage": "https://github.com/nicocube/wool",
  "repository": {
    "type": "git",
    "url": "git://github.com/nicocube/wool.git"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "0.10.*"
  },
  "scripts": {
    "start": "node server.js | bunyan",
    "watch": "nodemon -x 'node ./server.js | bunyan'",
    "pretest": "npm run lint",
    "test": "tape test/**/*.spec.js |tap-summary --no-progress",
    "autotest": "nodemon -e js,yml -x 'tape test/**/*.spec.js |tap-summary --no-progress'",
    "coverage": "istanbul cover tape test/**/*.spec.js",
    "lint": "eslint index.js lib/ test/",
    "autolint": "nodemon -x eslint index.js lib/ test/"
  },
  "dependencies": {
    "bankai": "^5.4.0",
    "bunyan": "^1.8.5",
    "knit": "0.2.*",
    "websocket": "^1.0.24",
    "wool-stream": "0.0.1"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.3",
    "istanbul": "^0.4.5"
  },
  "optionalDependencies": {}
}
